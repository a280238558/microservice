{
  "spring": {
    "application": {
      "name": "demo" //与配置文件的名称对应
    },
    "cloud": {
      "config": {
        "uri": "http://localhost:5100",
        "env": "dev" //与环境名称对应
      }
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
